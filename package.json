{
  "name": "pbsabella.github.io",
  "version": "1.0.0",
  "description": "Personal Website",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . && stylelint '**/*.css'",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint '**/*.css' --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "test:local": "start-server-and-test dev http://localhost:5173 cypress:run",
    "percy:test": "npm run build && start-server-and-test preview http://localhost:4173 'percy exec -- cypress run'",
    "percy:baseline": "PERCY_BRANCH=master percy exec -- cypress run --config baseUrl=https://pbsabella.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pbsabella/pbsabella.github.io.git"
  },
  "keywords": [
    "html",
    "css",
    "responsive"
  ],
  "author": "Paula Bianca Abella",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pbsabella/pbsabella.github.io/issues"
  },
  "homepage": "https://github.com/pbsabella/pbsabella.github.io#readme",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@percy/cli": "^1.28.0",
    "@percy/cypress": "^3.1.0",
    "@testing-library/cypress": "^10.1.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.9.0",
    "eslint": "^9.38.0",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.1.3",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
